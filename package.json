{
	"name": "my-next-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "start http://localhost:3000 && next dev",
		"server": "json-server --watch ./data/db.json --port 4000",
		"dev-server": "concurrently \"yarn dev\" \"yarn server\"",
		"build": "next build",
		"start": "next start",
		"new-posts": "cd ./data && node updatePosts.js new",
		"clear-posts": "cd ./data && node updatePosts.js clear"
	},
	"dependencies": {
		"concurrently": "^8.2.2",
		"dayjs": "^1.11.10",
		"next": "14.0.3",
		"next-auth": "^4.24.5",
		"react": "^18",
		"react-dom": "^18",
		"react-icons": "^4.12.0"
	},
	"devDependencies": {
		"@ngneat/falso": "^7.1.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"autoprefixer": "^10.0.1",
		"postcss": "^8",
		"tailwind-merge": "^2.1.0",
		"tailwindcss": "^3.3.0"
	}
}
